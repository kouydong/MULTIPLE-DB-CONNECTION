-- MSSQL 데이터 베이스, 테이블 및 데이터 추가
CREATE DATABASE MSSQL_TEST

USE MSSQL_TEST

CREATE TABLE MSSQL_TEST_TBL (
	USER_ID VARCHAR(50) PRIMARY KEY,
	USER_NAME VARCHAR(50) NOT NULL
)

INSERT INTO MSSQL_TEST_TBL VALUES('MSSQL_USER1', 'MSSQL_USER_NAME1')

INSERT INTO MSSQL_TEST_TBL VALUES('MSSQL_USER2', 'MSSQL_USER_NAME2')


-- MYSQL 데이터 베이스, 테이블 및 데이터 추가
CREATE DATABASE MYSQL_TEST

USE MYSQL_TEST

CREATE TABLE MYSQL_TEST_TBL (
	USER_ID VARCHAR(50) PRIMARY KEY,
	USER_NAME VARCHAR(50) NOT NULL
)

INSERT INTO MYSQL_TEST_TBL VALUES('MYSQL_USER', 'MYSQL_USER_NAME')

INSERT INTO MYSQL_TEST_TBL VALUES('MYSQL_USER2', 'MYSQL_USER_NAME2')